msgid "{i}искать под подушкой{/i}"
msgstr "{i}mirar debajo de la almohada{/i}"

# game/scripts/story_01/secretbooks.rpy:34
msgid "{i}искать под кроватью{/i}"
msgstr "{i}buscar debajo de la cama{/i}"

