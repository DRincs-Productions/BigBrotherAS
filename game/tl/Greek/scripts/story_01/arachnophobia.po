# game/scripts/story_01/arachnophobia.rpy:361 #-#-# translate crowdin ev_066_6f555264:
msgid "[_Max_s_16_] А нечего было меня будить..."
msgstr ""

# game/scripts/story_01/arachnophobia.rpy:366 #-#-# translate crowdin ev_066_0d35b189:
msgid "[_Alice_s_11_] Ладно, держи $10, только убери его быстрее!!!"
