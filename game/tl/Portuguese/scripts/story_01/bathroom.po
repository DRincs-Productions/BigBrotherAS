msgstr ""

# game/scripts/story_01/bathroom.rpy:226 #-#-# translate crowdin ev_ann_shower_caught_22cd2c10_1:
msgid "[_Max_s_13_] Хм... Как-то я даже не знаю..."
msgstr ""

# game/scripts/story_01/bathroom.rpy:227 #-#-# translate crowdin ev_ann_shower_caught_ad948cec_1:
