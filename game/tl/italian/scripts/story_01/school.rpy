translate italian ev_005_095efff2:

    # Max 11 "Так кто он?"
    Max 11 "Allora, chi è?"

# game/scripts/story_01/school.rpy:107
translate italian ev_005_08a94415:

    # Lisa 11 "А вот не скажу! Может быть, в другой раз..."
    Lisa 11 "Non te lo dico! Magari un'altra volta..."

