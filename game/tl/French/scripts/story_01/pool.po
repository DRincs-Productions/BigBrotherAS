msgstr "{i}maman admirative{/i}"

# game/scripts/story_01/pool.rpy:213
msgid "Конечно хочу!"
msgstr "Bien sûr que oui !"

# game/scripts/story_01/pool.rpy:220
msgid "{i}поплавать с мамой{/i}"
msgstr "{i}pour nager avec maman."

