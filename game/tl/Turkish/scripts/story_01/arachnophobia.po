# game/scripts/story_01/arachnophobia.rpy:346 #-#-# translate crowdin ev_066_eecd6573:
msgid "[_Max_s_13_] Ну, пойдём посмотрим..."
msgstr ""

# game/scripts/story_01/arachnophobia.rpy:353 #-#-# translate crowdin ev_066_5c4fe120:
msgid "[_Alice_s_14_] Макс, Макс! Вот он! Видишь? Убери его отсюда, скорее!!!"
